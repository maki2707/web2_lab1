<!doctype html>
<html lang="en">
  <%- include(`partials/head`); %>  
  <body>
    <%- include(`partials/navbar`); %> 
    <div>
      <div class="fixtures-header" >
        <a class="button-59" href="/fixtures/<%= idk-100%>" role="button">prethodno</a>
        <div class="fixture-title"><h1 style="font-weight: bolder;"><%= idk>1000 ? String(idk).slice(0, 2) :String(idk).slice(0, 1) ; %>.kolo</h1></div>
        <a class="button-59" href="/fixtures/<%= idk+100 %>" role="button"> sljedeÄ‡e</a>      
      </div>
     
        <% let kolo = idk %>
              <% console.log(kolo)%>
              <% for(var utakmica of raspored) { %>
                  <% console.log(utakmica) %>
                  <% if(utakmica.utakoloid === kolo) { %>
                    <div class="fixtures-header">
                    <div class="fixture">
                      <div class="fixture-row">
                        <div class="fixture-team"><%= utakmica.nazivtima %></div>
                        <div class="fixture-result1"><%= utakmica.goltima %></div>
                      </div>
                      <div class="fixture-row">
                        <div class="fixture-team"><%= utakmica.nazivtimb %></div>
                        <div class="fixture-result2"><%= utakmica.goltimb %></div>          
                      </div>
                    </div>
                  </div>
                    <% } %>
              <% } %>     
    </div>
    
    
    
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
  </body>
</html>